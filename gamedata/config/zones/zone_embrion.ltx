[zone_dog]
GroupControlSection	= spawn_group_zone
$spawn 			= "zones\mosquito bald"
$prefetch 		= 16
class			= ZS_MBALD
script_binding	     = bind_embrion.init

hit_impulse_scale	          = 0.0
effective_radius	          = 0.1 ;размер радиуса в процентах от оригинального, где действует зона
actor_blowout_radius_percent	= 0.0

ef_anomaly_type			= 1
ef_weapon_type			     = 13

sound			= anomaly\anomaly_body_tear_1

BirthProbability	= 0.0
hit_type			= telepatic

;----------- Anomaly settings -----------------------
min_start_power		= 0.10
max_start_power		= 0.15
attenuation			= 1
period				= 1
min_artefact_count	     = 0
max_artefact_count	     = 0

awaking_time		     = 100
blowout_time		     = 150	
accamulate_time		= 200

idle_particles			= anomaly2\body_tear_01

blowout_light       = off

disable_time		= 5000		;время игнорирования неживого объекта в зоне (-1 если не нужно)
disable_time_small	= 5000		;время игнорирования маленького неживого объекта в зоне (-1 если не нужно)
disable_idle_time	= 10		;время отключения idle партиклов

ignore_nonalive	= true
ignore_small		= true
ignore_artefacts	= true

blowout_light		= off

idle_light		= off

visible_by_detector = off

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;рождение артефактов 

artefacts	= 

;;во время срабатывания в online
spawn_blowout_artefacts    = off

artefact_spawn_probability = 0.0	;вероятность, что во время срабатывания аномалии будет рожден артефакт
artefact_spawn_particles   =		;партиклы, которые отыграются на месте рождения артефакта (опционально)
artefact_born_sound        =  	;звук, который отыграются на месте рождения артефакта (опционально)

throw_out_power            = 0	;импульс с которым артефакт будет выброшен из зоны в произвольном направлении
artefact_spawn_height      = 0	;(м) высота на центром зоны где будет рожден артефакт

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; ветер
blowout_wind		= off


