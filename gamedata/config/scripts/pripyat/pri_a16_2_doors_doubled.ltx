[logic]
active = ph_door@close

[ph_door@close]
locked = false
closed = true
on_info = {+pri_a16_2_doors_open} ph_door@open
on_use = %+pri_a16_2_doors_open%
hit_on_bone = 1|ph_door@free|2|ph_door@free

[ph_door@open]
locked = false
closed = false
on_info = {-pri_a16_2_doors_open} ph_door@close
on_use = %-pri_a16_2_doors_open%
hit_on_bone = 1|ph_door@free|2|ph_door@free

[ph_door@free]
locked = false
closed = true
no_force = true
on_use = ph_door@open

