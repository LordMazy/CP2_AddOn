[logic]
active = ph_door@locked

[ph_door@locked]
locked = true
closed = true
on_info = {+rad_code_door_unlocked} ph_door@close
snd_open_start = trader_door_locked

[ph_door@close]
locked = false
closed = true
on_use = ph_door@open

[ph_door@open]
locked = false
closed = false
on_use = ph_door@close
