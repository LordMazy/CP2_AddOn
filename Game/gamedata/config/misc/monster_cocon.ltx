[mutant_dog_cocoon]:caps_base
GroupControlSection   = spawn_group
$spawn                = "artifacts\mutant_dog_cocoon"
$prefetch            = 64
cform                 = skeleton
class				= ARTEFACT
visual                = physics\anomaly\cocoo8.ogf
description           = dog_cocoon_description
inv_name              = dog_cocoon
inv_name_short        = dog_cocoon
inv_weight            = 0.3
can_trade             = on
inv_grid_width        = 1
inv_grid_height       = 1
inv_grid_x            = 38
inv_grid_y            = 41
cost                  = 6300
jump_height		   = .6

particles		     = alwen\artefact_kaktus

lights_enabled 		= true
trail_light_color 	     = 0.9, 0.7, 0.5
trail_light_range 	     = 1.0

artefact_activation_seq	= embrion_activation
;скорости увеличения (уменьшения) 
health_restore_speed 	= 0.0			
radiation_restore_speed = 0.0
satiety_restore_speed 	= 0.0
power_restore_speed 	= 0.0
bleeding_restore_speed 	= 0.0
hit_absorbation_sect 	= mutant_cocoon_absorbation

hud			          = mutant_dog_cocoon_hud

[mutant_dog_cocoon_hud]
orientation             	= 0, 0, 0		
position                	= 0, 0, 0
visual                  	= aktiv_embrions\mutant_dog_hud.ogf
anim_idle					= idle
anim_idle_sprint			= idle_sprint
anim_hide					= holster
anim_show					= draw
anim_activate				= activate


